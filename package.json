{
  "name": "@jiabinbin/vben-components",
  "version": "0.1.1",
  "files": [
    "dist"
  ],
  "private": false,
  "main": "./dist/components/lib/index.umd.js",
  "module": "./dist/components/es/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/components/es/index.es.js",
      "require": "./dist/components/lib/index.umd.js"
    },
    "./dist/index.css": {
      "import": "./dist/components/es/style.css",
      "require": "./dist/components/lib/style.css"
    }
  },
  "types": "./dist/components/es/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "build:component": "yarn clean:lib && node scripts/build.js",
    "clean:lib": "rimraf dist",
    "lint:prettier": "prettier --write  \"(packages|docs)/**/*.{js,json,tsx,css,less,scss,vue,html,md,ts}\"",
    "publish:component": "npm publish --access public",
    "unpublish:component": "npm unpublish ant-design-vue-components --force"
  },
  "dependencies": {
    "@jiabinbin/vue3-library": "^0.0.13"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@nabla/vite-plugin-eslint": "^1.3.4",
    "@purge-icons/generated": "^0.7.0",
    "@types/lodash-es": "^4.17.5",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "@vueuse/core": "^6.8.0",
    "ant-design-vue": "^3.0.0-alpha.10",
    "eslint": "^8.2.0",
    "eslint-define-config": "^1.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "fs-extra": "^10.0.0",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "pinia": "^2.0.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-dts": "^0.9.3",
    "vite-plugin-svg-icons": "^1.0.5",
    "vue": "^3.2.16",
    "vue-tsc": "^0.3.0"
  },
  "peerDependencies": {
    "@vueuse/core": "^6.7.6",
    "ant-design-vue": "^3.0.0-alpha.10",
    "moment": "^2.29.1",
    "pinia": "^2.0.2",
    "vue": "^3.2.16"
  }
}
